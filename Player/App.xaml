<Application x:Class="Player.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:Player_Core="clr-namespace:Player" 
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./ViewStyle/CtlStyles.xaml" />
                <ResourceDictionary Source="./ViewStyle/DiyCMStyle.xaml" />
                <ResourceDictionary Source="./ViewStyle/SoundSlider.xaml" />
                <ResourceDictionary Source="./ViewStyle/NoResize_window.xaml"/>
                <ResourceDictionary Source="./ViewStyle/Hometab.xaml"/>
                <ResourceDictionary Source="./ViewStyle/TabItem.xaml"/>
                <ResourceDictionary Source="./ViewStyle/SerachTextBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Player_Core:PlayController x:Key="Player"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
